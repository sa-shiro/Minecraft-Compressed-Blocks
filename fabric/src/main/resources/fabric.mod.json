{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "${source}",
    "issues": "${source}/issues",
    "curseforge": "${curseforge_page}",
    "modrinth": "${modrinth_page}",
    "homepage": "${homepage}",
    "email": "${mod_author_email}"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/${logo_file}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.sashiro.compressedblocks.fabric.CompressedBlocksFabric"
    ],
    "client": [
      "net.sashiro.compressedblocks.fabric.client.CompressedBlocksClient"
    ],
    "fabric-datagen": [
      "net.sashiro.compressedblocks.fabric.data.CBFabricDataGenerator"
    ]
  },
  "depends": {
    "minecraft": "${fabric_mc_version_range}",
    "java": ">=21",
    "fabricloader": "${fabric_version_range}",
    "fabric-api": "*",
    "forgeconfigapiport": ">=21.1.3"
  },
  "suggests": {}
}
  